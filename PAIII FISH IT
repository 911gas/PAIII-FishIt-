-- PAIIII Speed Buttons
local PAIIII_SpeedButtons = {}
for i = 1, 4 do
    local PAIIII_Btn = Instance.new("TextButton")
    PAIIII_Btn.Size = UDim2.new(0.22, 0, 0, 25)
    PAIIII_Btn.Position = UDim2.new(0.03 + (0.24 * (i-1)), 0, 0.4, 0)
    PAIIII_Btn.BackgroundColor3 = PAIIIISpeeds[i].Color
    PAIIII_Btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    PAIIII_Btn.Text = PAIIIISpeeds[i].Name
    PAIIII_Btn.Font = Enum.Font.GothamBold
    PAIIII_Btn.TextSize = 11
    PAIIII_Btn.Parent = PAIIII_SpeedFrame
    
    local PAIIII_BtnCorner = Instance.new("UICorner")
    PAIIII_BtnCorner.CornerRadius = UDim.new(0, 4)
    PAIIII_BtnCorner.Parent = PAIIII_Btn
    
    PAIIII_Btn.MouseButton1Click:Connect(function()
        PAIIII.Speed = i
        PAIIII_UpdateSpeed()
    end)
    
    PAIIII_SpeedButtons[i] = PAIIII_Btn
end

