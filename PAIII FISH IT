    {Delay = 2.0, Name = "SLOW", Color = Color3.fromRGB(100, 200, 100)},
    {Delay = 1.0, Name = "NORMAL", Color = Color3.fromRGB(100, 150, 255)},
    {Delay = 0.5, Name = "FAST", Color = Color3.fromRGB(255, 200, 100)},
    {Delay = 0.2, Name = "ULTRA", Color = Color3.fromRGB(255, 50, 50)}
}

