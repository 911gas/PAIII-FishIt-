PAIIII_Method.Size = UDim2.new(0.9, 0, 0, 25)
PAIIII_Method.Position = UDim2.new(0.05, 0, 0.92, 0)
PAIIII_Method.BackgroundColor3 = Color3.fromRGB(80, 80, 200)
PAIIII_Method.TextColor3 = Color3.fromRGB(255, 255, 255)
PAIIII_Method.Text = "PAIIII METHOD: E KEY"
PAIIII_Method.Font = Enum.Font.GothamBold
PAIIII_Method.TextSize = 12
PAIIII_Method.Parent = PAIIII_Main

-- PAIIII Functions
function PAIIII_UpdateSpeed()
    for i, btn in ipairs(PAIIII_SpeedButtons) do
        if i == PAIIII.Speed then
            btn.BackgroundColor3 = Color3.fromRGB(255, 255, 100)
        else
            btn.BackgroundColor3 = PAIIIISpeeds[i].Color
        end
    end
end

function PAIIII_GetRealFish()
    local count = 0
    local backpack = player:FindFirstChild("Backpack")
    if backpack then
        for _, item in pairs(backpack:GetChildren()) do
            if item:IsA("Tool") and item.Name:lower():find("fish") then
                count = count + 1
            end
        end
    end
    return count
end

-- PAIIII Fishing Action
function PAIIII_Fish()
    local speed = PAIIIISpeeds[PAIIII.Speed]
    local method = PAIIIIMethods[PAIIII.Method]
    
    -- Cast
    if method.Key == "Mouse" then
        VIM:SendMouseButtonEvent(0, 0, 0, true, game, 0)
        wait(0.05)
        VIM:SendMouseButtonEvent(0, 0, 0, false, game, 0)
    else
        VIM:SendKeyEvent(true, method.Key, false, game)
        wait(0.05)
        VIM:SendKeyEvent(false, method.Key, false, game)
    end
    
    -- Wait
    wait(speed.Delay * 0.6)
    
    -- Reel
    if method.Key == "Mouse" then
        VIM:SendMouseButtonEvent(0, 0, 0, true, game, 0)
        wait(0.05)
        VIM:SendMouseButtonEvent(0, 0, 0, false, game, 0)
    else
        VIM:SendKeyEvent(true, method.Key, false, game)
        wait(0.05)
        VIM:SendKeyEvent(false, method.Key, false, game)
    end
    
    -- Update
    PAIIII.FishCount = PAIIII.FishCount + 1
    local realCount = PAIIII_GetRealFish()
    if realCount > PAIIII.RealFish then
        PAIIII.RealFish = realCount
    end
    
    PAIIII_Stats.Text = "PAIIII Fish: " .. PAIIII.FishCount .. " | Real: " .. PAIIII.RealFish
end

-- PAIIII Main Loop
function PAIIII_Loop()
    while PAIIII.Enabled do
        PAIIII_Fish()
        wait(PAIIIISpeeds[PAIIII.Speed].Delay * 0.4)
    end
end

-- PAIIII Button Events
PAIIII_Start.MouseButton1Click:Connect(function()
    if not PAIIII.Enabled then
        PAIIII.Enabled = true
        PAIIII_StatusLabel.Text = "PAIIII Status: FISHING âš¡"
        PAIIII_StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        PAIIII_Start.BackgroundColor3 = Color3.fromRGB(30, 120, 50)
        spawn(PAIIII_Loop)
    end
end)

PAIIII_Stop.MouseButton1Click:Connect(function()
    PAIIII.Enabled = false
    PAIIII_StatusLabel.Text = "PAIIII Status: STOPPED"
    PAIIII_StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    PAIIII_Start.BackgroundColor3 = Color3.fromRGB(60, 180, 80)
end)

PAIIII_Method.MouseButton1Click:Connect(function()
    PAIIII.Method = PAIIII.Method + 1
    if PAIIII.Method > #PAIIIIMethods then
        PAIIII.Method = 1
    end
    PAIIII_Method.Text = "PAIIII METHOD: " .. PAIIIIMethods[PAIIII.Method].Text
end)

-- PAIIII Hotkey
UIS.InputBegan:Connect(function(input, processed)
    if processed then return end
    if input.KeyCode == Enum.KeyCode.P then
        PAIIII.Enabled = not PAIIII.Enabled
        if PAIIII.Enabled then
            PAIIII_StatusLabel.Text = "PAIIII Status: FISHING âš¡"
            PAIIII_StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
            PAIIII_Start.BackgroundColor3 = Color3.fromRGB(30, 120, 50)
            spawn(PAIIII_Loop)
        else
            PAIIII_StatusLabel.Text = "PAIIII Status: STOPPED"
            PAIIII_StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
            PAIIII_Start.BackgroundColor3 = Color3.fromRGB(60, 180, 80)
        end
    end
end)

-- PAIIII Initialize
PAIIII_UpdateSpeed()
PAIIII.LastInventory = PAIIII_GetRealFish()
PAIIII.RealFish = PAIIII.LastInventory
PAIIII_Stats.Text = "PAIIII Fish: 0 | Real: " .. PAIIII.RealFish

print("âš¡ PAIIII FISHIT LOADED!")
